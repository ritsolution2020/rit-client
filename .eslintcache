) => {\r\n        if (success) {\r\n            return <h3 className=\"text-success\">{name} is created</h3>;\r\n        }\r\n    };\r\n\r\n    const showError = () => {\r\n        if (error) {\r\n            return <h3 className=\"text-danger\">Category should be unique</h3>;\r\n        }\r\n    };\r\n\r\n    const goBack = () => (\r\n        <div className=\"mt-5\">\r\n          \r\n            <div class=\"button_cont\"><a href=\"/admin/dashboard\" className=\"BackHome\" >\r\n            <i class=\"fas fa-angle-double-left\"></i>  Back to Dashboard </a>\r\n                \r\n                </div>\r\n        </div>\r\n    );\r\n\r\n    return (\r\n        <div>\r\n             \r\n            <div className=\"row\">\r\n                <div className=\"col-md-8 offset-md-2\">\r\n            {goBack()}\r\n                    {showSuccess()}\r\n                    {showError()}\r\n                    {newCategoryFom()}\r\n                    \r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default AddCategory;","C:\\Users\\USER\\Desktop\\port\\client\\src\\auth\\AdminRoute.js",["148"],"import React, { Component } from \"react\";\r\nimport { Route, Redirect } from \"react-router-dom\";\r\nimport { isAuthenticated } from \"./index\";\r\n\r\nconst AdminRoute = ({ component: Component, ...rest }) => (\r\n    <Route\r\n        {...rest}\r\n        render={props =>\r\n            isAuthenticated() && isAuthenticated().user.role === 1 ? (\r\n                <Component {...props} />\r\n            ) : (\r\n                <Redirect\r\n                    to={{\r\n                        pathname: \"/signin\",\r\n                        state: { from: props.location }\r\n                    }}\r\n                />\r\n            )\r\n        }\r\n    />\r\n);\r\n\r\nexport default AdminRoute;","C:\\Users\\USER\\Desktop\\port\\client\\src\\admin\\UpdateProject.js",["149","150","151","152","153","154"],"import React, { useState